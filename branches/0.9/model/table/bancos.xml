<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : bancos.xml
    Description:
        Estructura de la tabla bancos.
-->
<tabla>
   <columna>
      <nombre>codproveedor</nombre>
      <tipo>character varying(6)</tipo>
      <nulo>YES</nulo>
   </columna>
   <columna>
      <nombre>nombre</nombre>
      <tipo>character varying(100)</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>entidad</nombre>
      <tipo>character varying(4)</tipo>
      <nulo>NO</nulo>
   </columna>
   <restriccion>
      <nombre>bancos_pkey</nombre>
      <consulta>PRIMARY KEY (entidad)</consulta>
   </restriccion>
</tabla>